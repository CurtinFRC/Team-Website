{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "run dev",
      "type": "shell",
      "command": "yarn dev",
      "group": "build"
    },

    {
      "label": "git commit",
      "type": "shell",
      "command": "./.vscode/push.bat",
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }

    },

    {
      "label": "run build",
      "type": "shell",
      "command": "yarn build",
      "group": "build"
    }
  ],

}